@model OtobusBilet.Models.Kullanici
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Örnek Sayfa</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css">
    <style>

    </style>
</head>

<body>

    <h1>Kullanici Girişi:</h1>
    <hr />
    <form asp-controller="Kullanici" asp-action="KullaniciGiris2">

        <div class="form-group">
            <label for="KullaniciAdi">Kullanıcı Adı:</label>
            <input type="text" id="KullaniciAdi" name="KullaniciAdi" required asp-validation-for="KullaniciAdi" class="form-control" />
            <span asp-validation-for="KullaniciAdi" class="text-danger"></span>
        </div>
        <div class="form-group">
            <label for="Sifre">Şifre:</label>
            <input type="password" id="Sifre" name="Sifre" required asp-validation-for="Sifre" class="form-control" />
            <span asp-validation-for="Sifre" class="text-danger"></span>

        </div>
        <div class="form-group">
            <input type="submit" value="Giris" />
        </div>
    </form>

    <!-- Bootstrap 5 JS -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"></script>
    <!-- jQuery -->
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <script>


    </script>


</body>